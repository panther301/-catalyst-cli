{
  "name": "catalyst-cli",
  "version": "0.0.2",
  "author": "Anindya Giri",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:lib:dynamic-widget": "ng build dynamic-widget",
    "build:lib:stepper": "ng build stepper",
    "build:lib:tab": "ng build catalyst-tab",
    "build:lib:icon": "ng build catalyst-icon",
    "build:demo": "ng build --base-href /-catalyst-cli/",
    "predeploy": "npm run build:demo",
    "deploy": "npx angular-cli-ghpages --dir=dist/catalyst-cli/browser"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "peerDependencies": {
    "@angular/common": "^21.0.0",
    "@angular/core": "^21.0.0",
    "rxjs": "~7.8.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.4",
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "angular-cli-ghpages": "^2.0.3",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.0.1",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
